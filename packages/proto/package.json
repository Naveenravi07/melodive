{
  "name": "proto",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "codegen:ts": "mkdir -p gen/ts && npx protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=gen/ts --ts_proto_opt=esModuleInterop=true,forceLong=string,useOptionals=true -I . *.proto",
    "codegen:py": "bash ./scripts/codegen_py.sh",
    "codegen": "npm run codegen:ts && npm run codegen:py"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ts-proto": "^2.7.5"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.5.2"
  }
}
